{
  "name": "setique",
  "version": "1.0.0",
  "description": "The Niche Data Marketplace - Unique Data for Better AI",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "check-db": "node check-database.js",
    "postinstall": "echo 'Dependencies installed successfully'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:setup": "node scripts/setup-test-user.js",
    "test:a11y": "vitest run --grep a11y",
    "lighthouse": "lighthouse http://localhost:5173 --output html --output-path ./reports/lighthouse-report.html --chrome-flags=\"--headless\"",
    "security:scan": "echo 'Run: docker run -t owasp/zap2docker-stable zap-baseline.py -t http://host.docker.internal:5173' && echo 'Note: Requires Docker and running dev server'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.7.2",
    "lucide-react": "^0.545.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "stripe": "^14.10.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.10.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.2.3",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "vite": "^5.4.11",
    "vitest": "^3.2.4"
  }
}
